{
    "framework": "vite",
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "outputDirectory": "dist",
    "routes": [
      {
        "src": "/[^.]+",
        "dest": "/",
        "status": 200
      }
    ],
    "github": {
      "silent": true
    },
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  } 